<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <VBox layoutX="50.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <GridPane prefHeight="211.0" prefWidth="100.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Button fx:id="BtInt" mnemonicParsing="false" onMouseClicked="#btInt_accion" text="Intermedio" GridPane.rowIndex="2" />
                        <Button fx:id="BtPri" mnemonicParsing="false" onMouseClicked="#btPri_accion" text="Principiante" GridPane.rowIndex="1" />
                        <Button fx:id="BtExt" mnemonicParsing="false" onMouseClicked="#btExt_accion" text="Extremo" GridPane.rowIndex="3" />
                        <Button fx:id="BtPer" mnemonicParsing="false" onMouseClicked="#btPer_accion" text="Personalizado" GridPane.rowIndex="4" />
